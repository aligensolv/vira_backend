{
  "name": "express_blueprint",
  "version": "0.0.1",
  "scripts": {
    "setup": "tsc scripts/setup",
    "build": "tsc -p tsconfig.build.json && npm run copy:files",
    "start": "node dist/server.js",
    "dev:build": "tsc --watch",
    "copy:files": "ts-node scripts/copy-files.ts",
    "dev:run": "nodemon dist/server.js",
    "dev": "npm run copy:files && concurrently \"npm:dev:build\"  \"npm:dev:run\"",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "prepare": "husky",
    "db:migrate": "prisma migrate dev",
    "db:generate": "prisma generate",
    "db:reset": "prisma migrate reset --skip-generate",
    "postinstall": "prisma generate",
    "generate:package": "ts-node scripts/generate-package.ts",
    "generate:test": "ts-node scripts/generate-test.ts",
    "db:seed": "ts-node scripts/database_seeder_script.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.15.0",
    "argon2": "^0.44.0",
    "body-parser": "^2.2.0",
    "bullmq": "^5.58.5",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.2",
    "express": "^4.21.2",
    "express-rate-limit": "^8.0.1",
    "faker": "^6.6.6",
    "handlebars": "^4.7.8",
    "helmet": "^8.1.0",
    "ioredis": "^5.7.0",
    "isomorphic-dompurify": "^2.26.0",
    "jsonwebtoken": "^9.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.6",
    "rate-limit-redis": "^4.2.2",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "zod": "^4.1.5",
    "zx": "^8.8.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.34.0",
    "@faker-js/faker": "^10.0.0",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/fs-extra": "^11.0.4",
    "@types/handlebars": "^4.0.40",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/nodemailer": "^7.0.1",
    "@types/supertest": "^6.0.3",
    "@types/validator": "^13.15.3",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "cpx": "^1.5.0",
    "eslint": "^9.34.0",
    "globals": "^16.3.0",
    "husky": "^9.1.7",
    "jest": "^30.1.3",
    "jiti": "^2.5.1",
    "nodemon": "^3.1.10",
    "prisma": "^6.15.0",
    "rimraf": "^6.0.1",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.1",
    "tsup": "^8.5.0",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0"
  }
}
